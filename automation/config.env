# Server Configuration
PORT=5001

# File Upload Configuration
MAX_FILE_SIZE=10485760  # 10MB in bytes
UPLOAD_FOLDER=temp_uploads
RESULT_FOLDER=temp_results

# Excel Template Configuration - Fixed Cell Positions
# Format: FIELD_NAME_CELL=SHEET_NAME:ROW:COLUMN (1-based indexing)
TOTAL_AMOUNT_CELL=NSLJ TEMPLATE:19:15        # O19 - Total amount field
EMAIL_CELL=NSLJ TEMPLATE:3:5                 # E3 - Email field (row 3, column E)
COMPANY_ADDRESS_CELL=NSLJ TEMPLATE:2:2       # B2 - Company address field (row 2, column B)
ADDRESS_LINE1_CELL=NSLJ TEMPLATE:9:3         # C9 - First line of address (row 9, column C)
ADDRESS_LINE2_CELL=NSLJ TEMPLATE:10:3        # C10 - Second line of address (row 10, column C)
ITEM_NAME_CELL=NSLJ TEMPLATE:19:2            # B19 - Item name field (row 19, column B)
COMPANY_NAME_CELL=NSLJ TEMPLATE:1:2          # B1 - Company name field (row 1, column B)
POD_CELL=NSLJ TEMPLATE:12:13                 # M12 - Port of Destination field (merged cells M12-N12)

INVOICE_POSTAL_COUNTRY_CELL=NSLJ TEMPLATE:11:3 # C11 - Invoice postal code and country field (merged cells C11-D11)
INVOICE_EMAIL_CELL=NSLJ TEMPLATE:12:3        # C12 - Invoice email field (merged cells C12-D12)
INVOICE_PHONE_CELL=NSLJ TEMPLATE:13:3        # C13 - Invoice phone field (merged cells C13-D13)

# Export Certificate Excel Cell Mappings
EC_CHASSIS_NUMBER_CELL=NSLJ TEMPLATE:19:4    # D19 - Export certificate chassis number (row 19, column D)
EC_GASOLINE_CELL=NSLJ TEMPLATE:19:13         # M19 - Export certificate gasoline field (row 19, column M)
EC_SEAT_NUMBER_CELL=NSLJ TEMPLATE:19:14      # N19 - Export certificate seat number field (row 19, column N)
EC_WEIGHT_CELL=NSLJ TEMPLATE:19:11           # K19 - Export certificate weight field (row 19, column K)
EC_ENGINE_CAPACITY_CELL=NSLJ TEMPLATE:19:6  # F19 - Export certificate engine capacity (row 19, column F)
EC_LENGTH_CELL=NSLJ TEMPLATE:19:7           # G19 - Export certificate length (row 19, column G)
EC_WIDTH_CELL=NSLJ TEMPLATE:19:8           # H19 - Export certificate width (row 19, column H)
EC_HEIGHT_CELL=NSLJ TEMPLATE:19:9           # I19 - Export certificate height (row 19, column I)
EC_NET_WEIGHT_CELL=NSLJ TEMPLATE:19:10      # J19 - Export certificate net weight (row 19, column J)
EC_YEAR_CELL=NSLJ TEMPLATE:19:5             # E19 - Export certificate year field (row 19, column E)

